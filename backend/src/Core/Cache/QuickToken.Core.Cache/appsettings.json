{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    }
  },
  "WalletWorker": {
    "PrefetchCount": 1,
    "MaxDegreeOfParallelism": 1,
    "DelayOnEmptyBatchMs": "00:00:01",
    "CacheExpirationPeriod": "00:05:00"
  },
  "TransactionPolling": {
    "Interval": "00:00:01"
  }
}
